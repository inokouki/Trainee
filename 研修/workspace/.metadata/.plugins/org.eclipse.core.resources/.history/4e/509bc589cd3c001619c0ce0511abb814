package tutorial.entity;

import java.util.List;

import javax.annotation.Resource;

import org.seasar.struts.annotation.ActionForm;

import tutorial.form.BaridatorForm;

public class Baridator {

    @ActionForm
    @Resource
    protected BaridatorForm BaridatorForm;

    public static List<BaridatorForm> select() {

        StringBuilder sql = null;

        sql.append("SELECT * FROM seasar2tuto.baridators ");

        List<BaridatorForm> list = null ;

        return list;
    }
}